<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bglogin"
    tools:context=".IdOrderActivity">

    <ImageView
        android:id="@+id/img_back"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/ic_backblack"
        />

    <TextView
        android:id="@+id/tv_title"
        android:text="Home"
        app:layout_constraintTop_toTopOf="@id/img_back"
        app:layout_constraintBottom_toBottomOf="@id/img_back"
        app:layout_constraintStart_toEndOf="@id/img_back"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/robotobold"
        android:textColor="#121010"
        android:textSize="24sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order Berhasil Dibuat"
        android:fontFamily="@font/robotobold"
        android:textSize="24sp"
        android:layout_marginTop="33dp"
        android:textColor="#121010"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageView
        android:id="@+id/img_barcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_barcode"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@id/tv_status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/tv_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order ID : 1312431234"
        android:fontFamily="@font/robotobold"
        android:textSize="24sp"
        android:textColor="#121010"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/img_barcode"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />


    <TextView
        android:id="@+id/tv_kbn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kebun    :"
        android:textSize="18sp"
        android:fontFamily="@font/robotobold"
        android:textColor="#121010"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@id/tv_order"
        app:layout_constraintStart_toStartOf="@id/tv_order"
        />

    <TextView
        android:id="@+id/tv_nmkbn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kebun Magelang"
        android:textSize="18sp"
        android:fontFamily="@font/robotoregular"
        android:textColor="#121010"
        android:layout_marginTop="17dp"
        android:layout_marginStart="80dp"
        app:layout_constraintTop_toBottomOf="@id/tv_order"
        app:layout_constraintStart_toStartOf="@id/tv_order"
        />

    <TextView
        android:id="@+id/tv_kbn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jenis     :"
        android:textSize="18sp"
        android:fontFamily="@font/robotobold"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_kbn"
        app:layout_constraintStart_toStartOf="@id/tv_kbn"
        />

    <TextView
        android:id="@+id/tv_nmkbn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Daun Atsiri"
        android:textSize="18sp"
        android:fontFamily="@font/robotoregular"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nmkbn"
        app:layout_constraintStart_toStartOf="@id/tv_nmkbn"
        />

    <TextView
        android:id="@+id/tv_kbn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hasil      :"
        android:textSize="18sp"
        android:fontFamily="@font/robotobold"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_kbn2"
        app:layout_constraintStart_toStartOf="@id/tv_kbn2"
        />

    <TextView
        android:id="@+id/tv_nmkbn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100 Kg"
        android:textSize="18sp"
        android:fontFamily="@font/robotoregular"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nmkbn2"
        app:layout_constraintStart_toStartOf="@id/tv_nmkbn2"
        />

    <TextView
        android:id="@+id/tv_kbn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Waktu    :"
        android:textSize="18sp"
        android:fontFamily="@font/robotobold"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_kbn3"
        app:layout_constraintStart_toStartOf="@id/tv_kbn3"
        />

    <TextView
        android:id="@+id/tv_nmkbn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="09:30 WIB"
        android:textSize="18sp"
        android:fontFamily="@font/robotoregular"
        android:textColor="#121010"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nmkbn3"
        app:layout_constraintStart_toStartOf="@id/tv_nmkbn3"
        />






</androidx.constraintlayout.widget.ConstraintLayout>